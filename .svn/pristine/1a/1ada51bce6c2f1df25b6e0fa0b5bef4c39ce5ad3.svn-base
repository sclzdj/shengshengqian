    .block-content{
        height: 55rem;
        padding: 0px 20px;
    }
    .data-table-toolbar{
        margin-bottom: 0;
    }
    .find-content{
        width: 100%;height: auto;
    }
    .find-content-l{
        float: left;width: 360px;
        height: auto;
        position: relative;
        min-height: 50rem;
    }
    .find-content-r{
        float: left;width: 65%;height: auto;
    }
    .ip6-bg{
        position: absolute;
        left: 0px;
        top: 0px;
        width: 345px;
    }
    .ip6-ppcontent{
        margin: 87px 0px 0px 38px;
    }
    .ip6-ppcontent,.ip6-content{
         width: 270px;
        height: 482px;
    }
    .ip6-ppcontent{
        position: relative;
        overflow: hidden;
    }
    .ip6-pcontent{
        position: absolute; left: 0;
        overflow-x: hidden;
        overflow-y: scroll;
    }
     /* for Chrome */
    .ip6-pcontent::-webkit-scrollbar {
        display: none;
    }
    .ip6-content{
        background-color: #eee;
        width: 270px;
        height: 405px;
        /*position: relative;
        top: 87px;
        left: 38px;*/
    }
    .ip6-header{
        width: 270px;
        height: 33px;
    }
    .swiper-container{
        height: 96px;
    }
    .swiper-wrapper img{
        width: 270px;
        height: 96px;
    }
    .ip6-announcement{
        height: 30px;
        width: 270px;
        background-color: white;
        margin: 7px 0px;
    }
    .ip6-announcement-l{
        height: 30px;
        width: 15%;
        float: left;
        text-align: center;
    }
    .ip6-announcement-l-div{
        width: 100%;
        height: 26px;
        position: relative;
        top: 2px;
        border-right:1px solid #d9d9d9; 
        text-align: center;
        vertical-align: middle;
    }
    .ip6-announcement-l-div span{
        height: 100%;
        vertical-align: middle;
        display: inline-block;
    }
    .ip6-announcement-l-div img{
        width: 26px;
        vertical-align: middle;
    }
    .ip6-announcement-m{
        width: 85%;
        float: left;
    }
    .ip6-announcement-m span{
        font-size: 8px;
        line-height: 26px;
        color: #939393;
        padding-left: 5px;
    }
    .ip6-announcement-r{
        width: 10%;
        float: left;
    }
    .ip6-module{
        width: 100%;
        height: auto;
        min-height: 191px;
        display: table;
        background-color: white;
    }
    .ip6-arrange{
        width: 100%;
        margin-top: 2px;
        max-height: 70px;
    }
    .ip6-footer{
        margin-top: 405px;
        width: 100%;
    }
    .ip6-module-div{
        width: 33.33%;
        height: 64px;
        position: relative;
        float: left;
        border-right: 1px solid #eee;
        border-bottom: 1px solid #eee;
    }
    .corner{
        width: 20px !important;
        position: absolute;
        left: 69px;
    }
    .ip6-module-div img{
        width: 30px;
    }
    .ip6-module-img{
        height: 35px;
        text-align: center;
        padding: 5px 0 0px 0;
    }
    .ip6-title{
        -webkit-transform: scale(0.65);
        font-size: 12px;
        letter-spacing: 2px;
        text-align: center;
    }
    .ip6-title2{
        -webkit-transform: scale(0.5);
        letter-spacing: 2px;
        font-size: 12px;
        text-align: center;
        color: #c4c4c4;
        line-height: 0;
    }
    .editdiv{
        width: 100%;
        height: 630px;
        border: 1px solid #eee;
        border-top: 3px solid #e7eaec;
        margin-top: 15px;
    }
